{
  "name": "mastodon-custom-emoji-viewer",
  "version": "1.0.0",
  "author": "nzws <git@nzws.me>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint:js": "eslint --ignore-path ./.gitignore \"**/*.js\" && prettier --check --ignore-path ./.gitignore \"**/*.js\"",
    "lint:css": "stylelint '**/*.js'",
    "lint:md": "prettier --check --ignore-path ./.gitignore \"**/*.md\"",
    "lint:json": "prettier --check --ignore-path ./.gitignore \"**/*.json\"",
    "lint": "npm-run-all lint:* -s",
    "format:js": "prettier --write --ignore-path ./.gitignore \"**/*.js\"",
    "format:md": "prettier --write --ignore-path ./.gitignore \"**/*.md\"",
    "format:json": "prettier --write --ignore-path ./.gitignore \"**/*.json\"",
    "format": "npm-run-all format:* -s",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js"
  },
  "dependencies": {
    "linaria": "1.4.0-alpha.1",
    "polished": "3.4.1",
    "preact": "10.0.0-beta.3",
    "react-copy-to-clipboard": "5.0.1",
    "react-epic-spinners": "0.3.0",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/preset-env": "7.4.5",
    "autoprefixer": "9.6.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "core-js": "3.1.4",
    "css-loader": "3.0.0",
    "cssnano": "4.1.10",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "google-fonts-plugin": "4.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.7.0",
    "npm-run-all": "4.1.5",
    "postcss": "7.0.17",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "3.0.0",
    "terser-webpack-plugin": "1.3.0",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2",
    "webpack-merge": "4.2.1",
    "webpackbar": "3.2.0"
  }
}
